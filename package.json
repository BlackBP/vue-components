{
  "name": "vue-bbp-ui",
  "author": "blackbp",
  "homepage": "https://github.com/BlackBP/vue-dream-ui",
  "repository": {
    "type": "git",
    "url": "https://github.com/BlackBP/vue-dream-ui.git"
  },
  "description": "UI library based on VueJS framework",
  "keywords": [
    "vue",
    "ui"
  ],
  "version": "0.2.9",
  "private": false,
  "scripts": {
    "serve": "vue-cli-service serve src/app.js",
    "build": "vue-cli-service build",
    "build-bundle": "vue-cli-service build --target lib --name bbp-ui src/components.js.vue"
  },
  "main": "./src/components.js",
  "files": [
    "src/*",
    "*.json",
    "*.js"
  ],
  "dependencies": {
    "@mdi/font": "^3.5.95",
    "lodash": "^4.17.11",
    "normalize.css": "^8.0.1",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.4.0",
    "@vue/cli-service": "^3.4.0",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.6.10"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
