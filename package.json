  },
  "peerDependencies": {
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.4.0",
    "@vue/cli-service": "^3.4.0",
    "node-sass": "^4.9.0",
    "rollup": "^1.16.6",
    "rollup-plugin-commonjs": "^10.0.1",
